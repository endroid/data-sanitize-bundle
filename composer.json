{
    "name": "endroid/data-sanitize-bundle",
    "description": "Endroid Data Sanitize Bundle",
    "keywords": ["endroid", "data", "sanitize", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/data-sanitize-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "endroid/data-sanitize": "^1.6",
        "endroid/installer": "^1.2.2",
        "endroid/simple-spreadsheet": "^3.0",
        "symfony/config": "^5.4||^6.0",
        "symfony/dependency-injection": "^5.4||^6.0",
        "symfony/http-foundation": "^5.4||^6.0",
        "symfony/http-kernel": "^5.4||^6.0",
        "symfony/routing": "^5.4||^6.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\DataSanitizeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\DataSanitizeBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
