<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Data Sanitize{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ preload(asset('/build/base.css', 'endroid_data_sanitize')) }}">
    {% endblock %}
</head>
<body>
{% block body %}
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
        <a class="navbar-brand" href="{{ path('endroid_data_sanitize_merge_index') }}">Data Sanitize</a>
        <button class="navbar-toggler d-lg-none" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarsExampleDefault">
            {% block menu %}

            {% endblock %}
        </div>
    </nav>
    <div class="container-fluid">
        <div class="row">
            <main class="col-sm-12 col-md-12 pt-3" role="main">
                {% block content %}

                {% endblock %}
            </main>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ preload(asset('/build/manifest.js', 'endroid_data_sanitize')) }}"></script>
    <script src="{{ preload(asset('/build/base.js', 'endroid_data_sanitize')) }}"></script>
{% endblock %}
</body>
</html>